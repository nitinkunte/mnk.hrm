@page
@{
}
<h2>Create User</h2>
<form action="https://localhost:65201/api/identity/create" method="post">
    <table class="display" id="tbEmployees">
        <tr>
            <td>
                User Name:
            </td>

        </tr>
        <tr>
            <td>
                <input type="text" id="userName" name="userName" />
            </td>
        </tr>
        <tr>
            <td>
                Email:
            </td>
        </tr>
        <tr>
            <td>
                <input type="email" id="email" name="email" />
            </td>
        </tr>
        <tr>
            <td>
                Password:
            </td>
        </tr>
        <tr>
            <td>
                <input type="password" id="password" name="password" />
            </td>
        </tr>
        <tr>
            <td>
                <button type="reset">Reset</button>
                <button type="button" id="button" name="button">test</button>
            </td>
            <td>
                <button type="submit">Create</button>
            </td>
        </tr>
    </table>

</form>

@section Scripts {


<script type="text/javascript" language="javascript">
    $(document).ready(function() {
        $("#button").click(function(){
    $.get("https://localhost:65201/api/identity/", function(data, status){
        alert("Data: " + data + "\nStatus: " + status);
            });
        });
    });
</script>



}

